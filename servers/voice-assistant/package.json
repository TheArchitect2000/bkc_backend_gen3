{
  "name": "ia-voice-command-jovo",
  "version": "0.1.0",
  "private": true,
  "description": "A sample voice app that works with the Jovo Framework",
  "main": "src/index.js",
  "dependencies": {
    "alexa-verifier-middleware": "^1.0.2",
    "jovo-core": "^3.5.1",
    "jovo-db-filedb": "^3.5.1",
    "jovo-framework": "^3.5.2",
    "jovo-platform-alexa": "^3.5.4",
    "jovo-platform-dialogflow": "^3.5.2",
    "jovo-platform-googleassistant": "^3.5.4",
    "jovo-plugin-debugger": "^3.5.1",
    "node-rest-client": "^3.1.0",
    "node-rest-client-promise": "^3.1.1"
  },
  "devDependencies": {
    "gulp": "^4.0.0",
    "gulp-install": "^1.1.0",
    "gulp-run-command": "0.0.9",
    "gulp-zip": "^4.2.0",
    "jest": "^23.6.0"
  },
  "scripts": {
    "tsc": "node -v",
